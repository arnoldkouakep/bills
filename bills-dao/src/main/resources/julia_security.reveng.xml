<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
<type-mapping>
	<sql-type jdbc-type="ARRAY" hibernate-type="string"></sql-type>
</type-mapping>
<table-filter match-schema="security" match-name="level"/>
  <table-filter match-schema="security" match-name="role"/>
  <table-filter match-schema="security" match-name="user"/>
  <table-filter match-schema="security" match-name="widget"/>
</hibernate-reverse-engineering>